<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="MissingJavadocType" files=".*"/>
    <suppress checks="MissingJavadocMethod" files=".*"/>
    <suppress checks="AbbreviationAsWordInName" files=".*"/>
    <suppress checks="PackageName" files=".*"/>
    <suppress checks="CustomImportOrder" files=".*"/>
    <suppress checks="AvoidStarImport" files=".*"/>
</suppressions>